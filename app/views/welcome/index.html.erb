<h2>Dados de Venda</h2>


<% if user_signed_in? %>
  <ul>
    <a href="/vendas"><button type="button" class="button button1">Visualizar lista de compras </button></a>
  </ul>
<% else %>

  <div class="card border-success mb-3" >
    <div class="card-body text-center ">
      <ul>
        <%= link_to "logar", new_user_session_path, class: "btn btn-outline-success" %> <br><br>
        <%= link_to "novo usuario", new_user_registration_path, class: "btn btn-outline-success" %> <br><br>
        <%= link_to "Recuperar senha", new_user_password_path, class: "btn btn-outline-success" %> <br>
      </ul>
    </div>
  </div>

<% end %>
